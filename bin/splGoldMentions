#!/bin/bash

export CLASSPATH=""
for file in `ls dist`
do
  export CLASSPATH=$CLASSPATH:dist/$file
done
for file in `ls lib`
do
  export CLASSPATH=$CLASSPATH:lib/$file
done

java -Xmx1G -Djava.util.logging.config.file=logging.properties tasks.coref.spl.SPLCoreferencePipeline DATA/SPL/TEST/XML DATA/SPL/TEST/OUT_GOLD_EXP true

